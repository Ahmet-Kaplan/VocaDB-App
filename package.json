{
  "name": "VocaDB",
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "description": "VocaDB app is a mobile version of vocadb.net",
  "version": "2.0.2",
  "private": false,
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.12",
    "@storybook/addon-links": "^3.3.12",
    "@storybook/addons": "^3.3.12",
    "@storybook/react-native": "^3.3.12",
    "babel-core": "^6.26.0",
    "babel-plugin-root-import": "^5.1.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "flow-bin": "^0.59.0",
    "jest-cli": "^21.2.1",
    "prop-types": "^15.6.0",
    "react-dom": "16.0.0-beta.5",
    "react-native-storybook-loader": "^1.7.0",
    "react-test-renderer": "16.0.0-beta.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/VocaDB/VocaDB-App.git"
  },
  "keywords": [
    "vocadb"
  ],
  "author": "up2up",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VocaDB/VocaDB-App/issues"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "test": "node node_modules/jest/bin/jest.js --watchAll",
    "flow": "flow",
    "storybook": "storybook start -p 7007",
    "prestorybook": "rnstl"
